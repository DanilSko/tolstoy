    <choice>
     <reg>
      он
     </reg>
     <orig>
      онъ
     </orig>
    </choice>
    забылся и
    <choice>
     <reg>
      вдруг
     </reg>
     <orig>
      вдругъ
     </orig>
    </choice>
    <choice>
     <reg>
      вскочил
     </reg>
     <orig>
      вскочилъ
     </orig>
    </choice>
