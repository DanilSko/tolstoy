     <reg>
      он
     </reg>
     <orig>
      онъ
     </orig>
    </choice>
    <choice>
     <reg>
      упал
     </reg>
     <orig>
      упалъ
     </orig>
    </choice>
    со всего роста на землю и
    <hi rend="opdelimiter">
     <choice>
      <reg>
       зарыдал
      </reg>
      <orig>
       зарыдалъ
      </orig>
