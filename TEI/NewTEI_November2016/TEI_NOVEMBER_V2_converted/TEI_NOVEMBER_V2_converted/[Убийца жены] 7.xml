    <choice>
     <reg>
      Он
     </reg>
     <orig>
      Онъ
     </orig>
    </choice>
    <choice>
     <reg>
      подошел
     </reg>
     <orig>
      подошелъ
     </orig>
    </choice>
    <choice>
     <reg>
