    <choice>
     <reg>
      Грех
     </reg>
     <orig>
      Грѣхъ
     </orig>
    </choice>
    на
    <choice>
     <reg>
      тебе
     </reg>
     <orig>
      тебѣ
     </orig>
    </choice>
    большой, на
    <choice>
     <reg>
      мне
     </reg>
     <orig>
      мнѣ
     </orig>
    </choice>
